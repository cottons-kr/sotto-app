	"$schema": "https://repomix.com/schemas/latest/schema.json",
	"output": {
		"filePath": "./repomix-output.md",
		"style": "markdown",
		"removeEmptyLines": true
	},
	"ignore": {
		"useGitignore": true,
		"useDefaultPatterns": true,
		"customPatterns": [
			"src-tauri/gen/android/app/src/main/res/**/*.*",
			"src-tauri/gen/android/app/build/**/*.*",
			"**/*.pbxproj"
		]
	}
}
